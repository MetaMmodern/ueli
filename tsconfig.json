{
    "compilerOptions": {
        "target": "es2015",
        "module": "commonjs",
        "lib": ["dom", "ES2021"],
        "sourceMap": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "jsx": "preserve"
    }
}
